<%= header %>
%xh<%= t('guilds.title') %>%xn
<%= divider %>
<% guilds.each do |data| %>
<% if data['is_public'] == true %>
<%= left(%x10+%xn, 1) -%>
<% else %>
<%= left(%x9-%xn, 1) -%>
<% end %>
<%= left(data['abbr'], 4) -%>
<%= left(data['name'], 15) -%>
<%= left(data['type'],15) -%>
<%= left(data['status'],20) -%>
<%= left(data['desc'],15) -%>
<% end %>
<%= footer %>
