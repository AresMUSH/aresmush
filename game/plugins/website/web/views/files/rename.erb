<h1>Rename File</h1>


    <form method="POST" action="/file/rename?file=<%= @file %>">
        

    <p>
    <label for="name" class="col-xs-2 col-form-label">Name:</label>
    
    <input type="text" name="name" size="100" value="<%= @file %>"/>
    </p>
    

    <div class="row">
        <div class="col col-xs-12">
            <div class="pull-right">
    
        <p> <a href="/file/detail?file=<%= @file %>" class="btn btn-default">Cancel</a>  
            <input class="btn btn-primary" type="submit" value="Save"> </p>
    
</div>
</div>
</div>

    </form>


<hr/>

<img src="/files/<%= @file %>" class="image-container" />
