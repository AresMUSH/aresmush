<%= header %>
<% if paginator.out_of_bounds? %>
  <%= paginator.out_of_bounds_msg %>
<% else %>
    %xh<%= t('scenes.scenes_title')%>%xn

    <% paginator.page_items.each do |scene| %>
    <%= scene.id %> - <<%= privacy(scene) %>> <%= scene.title %> (<%= characters(scene) %>)
    <% end %>

    <%= line_with_text paginator.page_marker %>
<% end %>
<%= footer %>